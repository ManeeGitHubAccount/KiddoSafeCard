<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EducationActivity">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="295dp"
        android:layout_height="141dp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.525"
        app:srcCompat="@drawable/logo"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="624dp"
        android:gravity="center"
        android:text="Parent Educational Resources"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="DuplicateIds,HardcodedText" />

    <ImageButton
        android:id="@+id/btn_baby_wearing"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="20dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView_wearing"
        app:layout_constraintEnd_toStartOf="@+id/btn_baby_firstAid"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/baby_wearing"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btn_back"
        android:layout_width="47dp"
        android:layout_height="47dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:background="@drawable/back"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_baby_firstAid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.003"
        tools:ignore="ContentDescription,HardcodedText,VisualLintButtonSize" />

    <TextView
        android:id="@+id/textView_wearing"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginBottom="36dp"
        android:gravity="center"
        android:text="Baby Wearing"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_baby_diapering"
        app:layout_constraintEnd_toStartOf="@+id/textView_firstAid"
        app:layout_constraintHorizontal_bias="0.405"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="DuplicateIds,HardcodedText" />

    <ImageButton
        android:id="@+id/btn_baby_firstAid"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="20dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView_firstAid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.871"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/baby_first_kid"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView_firstAid"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginBottom="36dp"
        android:gravity="center"
        android:text="Babies first kit"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_baby_bond"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="DuplicateIds,HardcodedText" />

    <ImageButton
        android:id="@+id/btn_baby_bond"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="24dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView_babybond"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/bond_baby"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView_babybond"
        android:layout_width="157dp"
        android:layout_height="80dp"
        android:layout_marginBottom="76dp"
        android:gravity="center"
        android:text="Bond with your baby"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_baby_diapering"
        tools:ignore="DuplicateIds,HardcodedText" />

    <ImageButton
        android:id="@+id/btn_baby_diapering"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="20dp"
        android:contentDescription="TODO"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView_baby_diapering"
        app:layout_constraintEnd_toStartOf="@+id/btn_baby_bond"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/learn_diapering_teq"
        tools:ignore="ContentDescription,HardcodedText" />
    <TextView
        android:id="@+id/textView_baby_diapering"
        android:layout_width="168dp"
        android:layout_height="84dp"
        android:layout_marginBottom="76dp"
        android:gravity="center"
        android:text="Learn Diapering Techniques"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="DuplicateIds,HardcodedText" />


</androidx.constraintlayout.widget.ConstraintLayout>