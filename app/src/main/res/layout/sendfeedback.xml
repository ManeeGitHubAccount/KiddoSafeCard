<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/rectangle3"
        android:layout_width="440dp"
        android:layout_height="120dp"
        android:background="#948A8A"
        tools:layout_editor_absoluteX="-15dp"
        tools:layout_editor_absoluteY="-6dp"
        tools:ignore="MissingConstraints,VisualLintBounds" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:text="How likely are you to recommend our app to others?"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        tools:ignore="DuplicateIds,HardcodedText,MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="15dp" />

    <View
        android:id="@+id/rectangle2"
        android:layout_width="440dp"
        android:layout_height="420dp"
        android:background="#FB5E1B"
        tools:ignore="MissingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="-24dp"
        tools:layout_editor_absoluteY="106dp" />

    <ImageButton
        android:id="@+id/btn_img_verygood"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginStart="32dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/very_good"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteY="130dp" />

    <TextView
        android:id="@+id/textView_verygood"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Very Good"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.368"
        tools:ignore="DuplicateIds,HardcodedText,VisualLintOverlap" />

    <ImageButton
        android:id="@+id/btn_img_good"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginStart="208dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/good"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteY="130dp" />


    <TextView
        android:id="@+id/textView_good"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Good"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.855"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.367"
        tools:ignore="DuplicateIds,HardcodedText" />

    <ImageButton
        android:id="@+id/btn_img_fair"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginStart="32dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/fair"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteY="329dp" />

    <TextView
        android:id="@+id/textView_fair"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Fair"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.144"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.628"
        tools:ignore="DuplicateIds,HardcodedText" />

    <ImageButton
        android:id="@+id/btn_img_poor"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginStart="200dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/poor"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteY="329dp" />

    <TextView
        android:id="@+id/textView_poor"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Poor"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.851"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.616"
        tools:ignore="DuplicateIds,HardcodedText,VisualLintOverlap" />

    <EditText
        android:id="@+id/edit_dob"
        android:layout_width="357dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/edittext_background"
        android:ems="10"
        android:hint="Your feedback"
        android:inputType="date"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="Autofill,HardcodedText,MissingConstraints,TextFields"
        tools:layout_editor_absoluteY="574dp" />

    <Button
        android:id="@+id/btn_send_feedback"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/background_btn_1"
        android:text="Send Feedback"
        android:textColor="@color/white"
        android:textSize="25sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="696dp" />


</androidx.constraintlayout.widget.ConstraintLayout>